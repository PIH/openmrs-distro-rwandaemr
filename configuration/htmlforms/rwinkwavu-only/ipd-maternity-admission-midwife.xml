        if ((birthYear / 4) == (Math.round(birthYear / 4))) {
        countLeap = 29}
        else {
        countLeap = 28}

        <!-- Calculate the days in the month -->
        if (birthMonth == 2) {
        countMonth = countLeap}
        else {
        if (birthMonth == 4) {
        countMonth = 30}
        else {
