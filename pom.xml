<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>org.openmrs.distro</groupId>
    <artifactId>rwandaemr-imb</artifactId>
    <version>2.0.0-SNAPSHOT</version>
    <packaging>pom</packaging>
    <name>RwandaEMR - IMB</name>
    <description>IMB Distribution of the Rwanda EMR</description>

    <modules>
        <module>rwandaemr-imb-butaro</module>
        <module>rwandaemr-imb-rwinkwavu</module>
    </modules>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <openMRSVersion>2.3.4-SNAPSHOT</openMRSVersion>
        <addresshierarchyVersion>2.14.2</addresshierarchyVersion>
        <calculationVersion>1.2.1</calculationVersion>
        <formconceptsVersion>2.0.0-SNAPSHOT</formconceptsVersion>
        <idgenVersion>4.6.0</idgenVersion>
        <groovyVersion>2.3.0</groovyVersion>
        <heightweighttrackerVersion>2.0.0-SNAPSHOT</heightweighttrackerVersion>
        <htmlformentryVersion>4.2.0-SNAPSHOT</htmlformentryVersion>
        <htmlformflowsheetVersion>2.0.0-SNAPSHOT</htmlformflowsheetVersion>
        <htmlwidgetsVersion>1.10.0</htmlwidgetsVersion>
        <initializerVersion>2.1.0-SNAPSHOT</initializerVersion>
        <laboratorymanagementVersion>2.0.0-SNAPSHOT</laboratorymanagementVersion>
        <legacyuiVersion>1.9.0</legacyuiVersion>
        <metadatamappingVersion>1.3.5</metadatamappingVersion>
        <metadatasharingVersion>1.7.0</metadatasharingVersion>
        <mohappointmentVersion>2.0.0-SNAPSHOT</mohappointmentVersion>
        <mohbillingVersion>2.0.0-SNAPSHOT</mohbillingVersion>
        <mohtracVersion>2.0.0-SNAPSHOT</mohtracVersion>
        <namephoneticsVersion>1.18.0-SNAPSHOT</namephoneticsVersion>
        <omodExportVersion>2.0.0-SNAPSHOT</omodExportVersion>
        <orderextensionVersion>3.0.0-SNAPSHOT</orderextensionVersion>
        <patientsummaryVersion>1.4.0</patientsummaryVersion>
        <pharmacymanagementVersion>2.0.0-SNAPSHOT</pharmacymanagementVersion>
        <reportingcompatibilityVersion>2.0.6</reportingcompatibilityVersion>
        <reportingobjectgroupVersion>2.0.0-SNAPSHOT</reportingobjectgroupVersion>
        <reportingVersion>1.22.0-SNAPSHOT</reportingVersion>
        <rowperpatientreportsVersion>2.0.0-SNAPSHOT</rowperpatientreportsVersion>
        <rwandahivflowsheetVersion>2.0.0-SNAPSHOT</rwandahivflowsheetVersion>
        <rwandaprimarycareVersion>2.1.0-SNAPSHOT</rwandaprimarycareVersion>
        <rwandareportsVersion>3.0.0-SNAPSHOT</rwandareportsVersion>
        <serializationVersion>0.2.14</serializationVersion>
        <simplelabentryVersion>2.0.0-SNAPSHOT</simplelabentryVersion>
        <syncVersion>2.0.0-SNAPSHOT</syncVersion>
        <usagestatisticsVersion>2.0.0-SNAPSHOT</usagestatisticsVersion>
        <validationVersion>2.0.0-SNAPSHOT</validationVersion>
        <webservicesRestVersion>2.29.0</webservicesRestVersion>
        <pathologyappVersion>1.0.3</pathologyappVersion>
        <legacyuinavbarappVersion>1.0.0</legacyuinavbarappVersion>
        <imbpathologyrequestVersion>1.0.0-SNAPSHOT</imbpathologyrequestVersion>
        <spamodulesVersion>1.0.9-SNAPSHOT</spamodulesVersion>
        <reportingrestVersion>1.13.0-SNAPSHOT</reportingrestVersion>
    </properties>

    <scm>
        <developerConnection>scm:git:https://github.com/pih/openmrs-distro-rwandaemr</developerConnection>
        <url>https://github.com/pih/openmrs-distro-rwandaemr</url>
        <tag>HEAD</tag>
    </scm>

    <repositories>
        <repository>
            <id>openmrs-repo</id>
            <name>OpenMRS Nexus Repository</name>
            <url>https://openmrs.jfrog.io/artifactory/public</url>
        </repository>
        <repository>
            <id>rwandaemr-repo-snapshots</id>
            <name>RwandaEMR Artifactory Snapshots Repository</name>
            <url>https://rwandaemr.jfrog.io/artifactory/libs-snapshot</url>
        </repository>
        <repository>
            <id>rwandaemr-repo-releases</id>
            <name>RwandaEMR Artifactory Release Repository</name>
            <url>https://rwandaemr.jfrog.io/artifactory/libs-release</url>
        </repository>
        <repository>
            <id>openmrs-repo-modules-pih</id>
            <name>OpenMRS PIH Modules</name>
            <url>https://openmrs.jfrog.io/artifactory/modules-pih/</url>
        </repository>
        <repository>
            <id>openmrs-repo-modules-pih-snapshots</id>
            <name>OpenMRS PIH Module Snapshots</name>
            <url>https://openmrs.jfrog.io/artifactory/modules-pih-snapshots/</url>
        </repository>
    </repositories>

    <pluginRepositories>
        <pluginRepository>
            <id>openmrs-repo</id>
            <name>OpenMRS Nexus Repository</name>
            <url>https://openmrs.jfrog.io/artifactory/public</url>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </pluginRepository>
    </pluginRepositories>

    <distributionManagement>
        <repository>
            <id>openmrs-repo-modules-pih</id>
            <name>OpenMRS PIH Modules</name>
            <url>https://openmrs.jfrog.io/artifactory/modules-pih/</url>
        </repository>
        <snapshotRepository>
            <id>openmrs-repo-modules-pih-snapshots</id>
            <name>OpenMRS PIH Module Snapshots</name>
            <url>https://openmrs.jfrog.io/artifactory/modules-pih-snapshots/</url>
        </snapshotRepository>
    </distributionManagement>

</project>
