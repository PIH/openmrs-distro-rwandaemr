    <script type="text/javascript">

        $j(document).ready(function () {
        // Hide elements by grouping them into arrays
        function hideElements(elementIds) {
        elementIds.forEach(function (id) {
        $j(id).hide();
        });
        }

        // Initial hide for sections
        hideElements([
        '#cough1', '#cough2', '#cough3', '#cough4', '#cough5', '#cough6', '#classbreathid',
        '#nosifDrug', '.bloodyDia', '#diarrh1', '#diarrh2', '#diarrh3', '#diarrh4', '#diarrh5', '#diarrh6',
        '#diarrh7', '#diarrh8', '#diarrh9', '#diarrh10', '#diarrh11', '#classdiarrhid',
        '#fever0', '#fever1', '#fever2', '#fever3', '#fever4', '#fever5', '#fever6', '#fever7',
        '#fever8', '#fever9', '#fever10', '#fever11', '#fever12', '#fever13', '#fever14', '#fever15',
        '#classfeverid', '.eartr', '.tb'
        ]);

        // Toggle visibility based on radio buttons
        function toggleSection(sectionId, show) {
        if (show) {
        $j(sectionId).show();
        } else {
        $j(sectionId).hide();
        }
        }

        // Event handler for showing sections based on checkbox state
        function handleToggleChange(selector, sectionIds) {
        $j(selector).change(function () {
        var isCheckedYes = $j(this).find('input[value="1066"]').prop('checked');
        sectionIds.forEach(function (id) {
        toggleSection(id, isCheckedYes);
        });
        });
        }

        // Apply toggle to different sections
        handleToggleChange('#coughid', ['#cough1', '#cough2', '#cough3', '#cough4', '#cough5', '#cough6', '#classbreathid']);
        handleToggleChange('#diarrhoeaid', ['#diarrh1', '#diarrh2', '#diarrh3', '#diarrh4', '#diarrh5', '#diarrh6', '#diarrh7', '#diarrh8', '#diarrh9', '#diarrh10', '#diarrh11', '#classdiarrhid']);
        handleToggleChange('#feverid', ['#fever0', '#fever1', '#fever2', '#fever3', '#fever4', '#fever5', '#fever6', '#fever7', '#fever8', '#fever9', '#fever10', '#fever11', '#fever12', '#fever13', '#fever14', '#fever15', '#classfeverid']);
        handleToggleChange('#earid', ['.eartr']);
        handleToggleChange('#tbrisk', ['.tb']);

        // Toggle bloody diarrhea section based on stool bloody input
        $j('#stoolBloody input').change(function () {
        toggleSection('.bloodyDia', $j(this).val() === "1065");
        });

        // Toggle nosifDrug section based on harmful input
        $j('#harmful input').change(function () {
        toggleSection('#nosifDrug', $j(this).val() === "1065");
        });

        // Automatically check anemia based on other anemia selections
        function autoCheckAnemia(triggerId, targetSelector, value) {
        $j(triggerId).click(function () {
        $j(targetSelector).prop("checked", $j(this).find(`input[value="${value}"]`).prop('checked'));
        });
        }

        autoCheckAnemia('#anemia1', '#anemia4 input[value="8618"]', "111005");
        autoCheckAnemia('#anemia2', '#anemia5 input[value="10988"]', "112235");
        autoCheckAnemia('#anemia3', '#anemia6 input[value="110883"]', "112113");

        // Toggle diaHidClass based on diarrhea duration
        $j("#diaDays input").change(function () {
        var diaDuration = parseInt($j(this).val());
        toggleSection('.diaHidClass', diaDuration >= 14);
        });


