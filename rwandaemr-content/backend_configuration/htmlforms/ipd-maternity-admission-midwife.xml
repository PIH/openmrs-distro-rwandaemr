    <script type="text/javascript">
        var $j = jQuery.noConflict();

        $j(document).ready(function () {
        // Listen for changes in the LMP field
        getField('lmp.value').change(function () {
        var mp = getValue('lmp.value'); // Get the LMP value
        if (!mp) return; // Exit if no LMP is entered

        var lmpDate = new Date(mp); // Convert LMP to a Date object

        // Calculate EDD: Add 1 year, subtract 3 months, add 7 days
        var eddDate = new Date(lmpDate);
        eddDate.setFullYear(eddDate.getFullYear() + 1);
        eddDate.setMonth(eddDate.getMonth() - 3);
        eddDate.setDate(eddDate.getDate() + 7);

        // Update the EDD field
        var eddFormatted = eddDate.toISOString().split('T')[0]; // Format as YYYY-MM-DD
        setValue('dd.value', eddFormatted);

        // Calculate gestational age (weeks and days)
        var today = new Date();
        var diffInMs = today - lmpDate; // Difference in milliseconds
        var diffInDays = Math.floor(diffInMs / (1000 * 60 * 60 * 24)); // Convert to days

        if (diffInDays &lt; 0) {
        setValue('wkId.value', 0);
        setValue('dayId.value', 0);
        return;
        }

        var weeks = Math.floor(diffInDays / 7); // Calculate completed weeks
        var days = diffInDays % 7; // Remaining days

        // Calculate remaining weeks until 36 weeks (if needed)
        var remainingWeeks = 36 - weeks;
        if (remainingWeeks &lt; 0) remainingWeeks = 0;

        // Update gestational age fields
        setValue('wkId.value', weeks); // Completed weeks
        setValue('dayId.value', days); // Remaining days within the current week

        // Optional: Display a message
        var message = weeks + " weeks and " + days + " days gestational age.";
        $j('#wkmsg').text(message).css('color', 'blue').css("font-weight", "Bold");
        });
        });
    </script>

